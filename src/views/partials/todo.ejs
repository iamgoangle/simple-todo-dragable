<h3>Important</h3>
<ol class="my_todo_app_star vertical">
  <li>
    <div class="panel panel-default panel-todo">
      <div class="panel-body">
        <div class="col-md-1">
          <span class="glyphicon glyphicon-star pin-todo" aria-hidden="true"></span>
        </div>
        <div class="col-md-2">Date</div>
        <div class="col-md-8">Item 1</div>
        <div class="col-md-1">
          <button type="button" class="btn btn-success">Done</button>
        </div>
      </div>
    </div>
  </li>
</ol>

<h3>My Todo</h3>
<ol class="my_todo_app vertical">
  <% for(var i=0; i < todos.length; i++) { %>
    <li>
      <div class="panel panel-default panel-todo">
        <div class="panel-body">
          <div class="col-md-1">
            <span class="glyphicon glyphicon-star" aria-hidden="true"></span>
          </div>
          <div class="col-md-2">
            <%= todos[i].date %>
          </div>
          <div class="col-md-8">
            <%= todos[i].task + todos[i].id %>
          </div>
          <div class="col-md-1">
            <button type="button" class="btn btn-success">Done</button>
          </div>
        </div>
      </div>
    </li>
  <% } %>
